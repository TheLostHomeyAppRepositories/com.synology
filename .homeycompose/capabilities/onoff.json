{
  "type": "boolean",
  "title": {
    "en": "Turned on",
    "nl": "Aangezet",
    "de": "Eingeschaltet"
  },
  "getable": true,
  "setable": true,
  "insights": true,
  "insightsTitleTrue": {
    "en": "Turned on",
    "nl": "Aangezet",
    "de": "Eingeschaltet"
  },
  "insightsTitleFalse": {
    "en": "Turned off",
    "nl": "Uitgezet",
    "de": "Ausgeschaltet"
  },
  "options": {
    "setOnDim": {
      "type": "boolean",
      "default": true
    },
    "greyout": {
      "type": "boolean",
      "default": true,
      "desc": {
        "en": "When this capability is false, give a visual hint to greyout the device.",
        "de": "wenn diese Fähigkeit 'false' ist, gibt es einen visuellenen Hinweis, um das Gerät auszugrauen."
      }
    }
  },
  "uiComponent": "toggle",
  "uiQuickAction": false,
  "$flow": {
    "triggers": [
      {
        "id": "onoff_true",
        "title": {
          "en": "Turned on",
          "nl": "Aangezet ",
          "de": "Angeschaltet"
        }
      },
      {
        "id": "onoff_false",
        "title": {
          "en": "Turned off",
          "nl": "Uitgezet",
          "de": "Ausgeschaltet"
        }
      }
    ],
    "conditions": [
      {
        "id": "on",
        "title": {
          "en": "Is turned !{{on|off}}",
          "nl": "Is !{{aan|uit}}",
          "de": "Ist !{{an|aus}}"
        }
      }
    ],
    "actions": [
      {
        "id": "on",
        "title": {
          "en": "Turn on",
          "nl": "Zet aan",
          "de": "Einschalten"
        }
      },
      {
        "id": "off",
        "title": {
          "en": "Turn off",
          "nl": "Zet uit",
          "de": "Ausschalten"
        }
      },
      {
        "id": "toggle",
        "title": {
          "en": "Toggle on or off",
          "nl": "Schakel aan of uit",
          "de": "Ein- oder ausschalten"
        }
      }
    ]
  }
}